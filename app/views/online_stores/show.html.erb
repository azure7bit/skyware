<div class="container">
      <div class="row row-offcanvas row-offcanvas-right">
        <div class="col-xs-12 col-sm-9">
          <div class="jumbotron">
            <p>
              <b>Super admin:</b>
              <%= @online_store.super_admin_id %>
            </p>
            <ul>
            	<% @products.each do |product| %>
            		<li><%= link_to "#{product.name}", online_store_product_path(product) %></li>
            	<% end %>
            </ul>

             </div>
          
        </div><!--/span-->

        <div class="col-xs-6 col-sm-3 sidebar-offcanvas" id="sidebar" role="navigation">
          <div class="well sidebar-nav">
            <ul class="nav">
              <li>Navigation</li>
              <li>
                <%= link_to "Add Products", new_online_store_product_path(@online_store) %>
                <%= link_to 'Edit', edit_online_store_path(@online_store) %> |
                <%= link_to 'Back', online_stores_path %>
              </li>
            </ul>
          </div><!--/.well -->
        </div><!--/span-->
      </div><!--/row-->

</div><!--/.container-->