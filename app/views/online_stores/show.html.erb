<div class="container">
    <div class="row">
        <div class="col-md-3">
            <div class="well sidebar-nav">
                <ul class="nav">
                    <li>
                        Navigation
                    </li>
                    <li>
                        <%= link_to "Add Products", new_online_store_product_path(@online_store) %>
                        <%= link_to 'Edit', edit_online_store_path(@online_store) %>
                        <%= link_to 'Back', online_stores_path %>
                    </li>
                </ul>
            </div>
        </div>
        <!-- End Sidebar --><!-- Begin Content -->
        <div class="col-md-9">
            <div class="row margin-bottom-10">
                <p>
                    <b>Super admin:</b>
                    <%= @online_store.super_admin_id %>
                </p>
                <ul>
                    <% @products.each do |product| %>
                    <li>
                        <%= link_to "#{product.name}", online_store_product_path(product) %>
                    </li>
                    <% end %>
                </ul>
            </div>
        </div>
    </div>
</div>
