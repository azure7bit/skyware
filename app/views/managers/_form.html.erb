<%= simple_form_for([@business, @manager]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :email %>
  	<%= f.input :password %>
  	<%= f.input :password_confirmation %>
  	<%= f.input :first_name %>
  	<%= f.input :last_name %>
  	<%= f.input :business_id %>
  	<%= f.input :title %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
