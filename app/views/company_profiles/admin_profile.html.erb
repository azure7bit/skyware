<%= render "layouts/partials/header" %>

<div class="container">
      <div class="row row-offcanvas row-offcanvas-right">
        <div class="col-xs-12 col-sm-9">
          <div class="jumbotron">
              <h2>
                <div>
                    <%= @super_admin.name %>
                </div>
              </h2>
              <h2>
                You've been a SkyHQ member for <%= time_ago_in_words(@super_admin.created_at) %>.
              </h2>
              <h2>
                Location: <%= @super_admin.location %><br />
                Phone Number: <%= @super_admin.phone_number %>
              </h2>
             </div>
          
        </div><!--/span-->

        <div class="col-xs-6 col-sm-3 sidebar-offcanvas" id="sidebar" role="navigation">
          <div class="well sidebar-nav">
            <ul class="nav">
              <li>Navigation</li>
              <li>
                <a href="#">Edit</a>
              </li>
            </ul>
          </div><!--/.well -->
        </div><!--/span-->
      </div><!--/row-->

</div><!--/.container-->
