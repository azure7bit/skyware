<%= simple_form_for(@company_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :super_admin_id %>
    <%= f.input :company_name %>
    <%= f.input :website %>
    <%= f.input :hq_street_address %>
    <%= f.input :hq_city %>
    <%= f.input :hq_state %>
    <%= f.input :hq_zipcode %>
    <%= f.input :hq_country %>
    <%= f.input :company_establishment_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
