<%= simple_form_for([@company_profile], :defaults => { :wrapper_html => {:class => 'form-group'}, :input_html => { :class => 'form-control' } }) do |f| %>
<%= f.error_notification %>
<div class="form-inputs">
    <%= f.input :business_user_id, :as => :hidden, :value => @company_profile.business_user_id %>
    <%= f.input :company_name %>
    <%= f.input :website %>
    <%= f.input :hq_street_address %>
    <%= f.input :hq_city %>
    <%= f.input :hq_state %>
    <%= f.input :hq_zipcode %>
    <%= f.input :hq_country %>
    <%= f.input :company_establishment_date, as: :date %>
</div>
<%= f.button :submit, :class => "btn btn-primary btn-large" %>
<% end %>
