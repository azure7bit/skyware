<!DOCTYPE html>
<html>
<head>
  <title>SkyhqNew</title>
  <%= stylesheet_link_tag    "devise", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% flash.each do |key, value| %>
    <div id="alert">
      <%= content_tag :div, value, :class => "alert flash #{key}" unless value.blank? %>
    </div>
  <% end %>
  
  <%= yield %>

</body>
</html>
