<!DOCTYPE html>
<html>
<head>
  <title>SkyhqNew</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render "layouts/partials/header" %>
  
  <% flash.each do |key, value| %>
    <div id="alert">
      <%= content_tag :div, value, :class => "alert flash #{key}" unless value.blank? %>
    </div>
  <% end %>
  <div class="container">
    <%= yield %>
  </div>
  <footer>
    <%= yield :scripts %>
  </footer>
</body>
</html>
