<!DOCTYPE html>
<html lang="en">
<head>
  <title>Skywyre</title>
  <%= meta_tags                %>
  <%= favicon                  %>
  <%= stylesheets :application %>
  <%= javascripts :application %>
  <%= google_analytics         %>
  <%= yield(:head)             %>
